{
  "name": "@gravitywelluk/eslint-plugin",
  "description": "Gravitywell defined ESLint rule sets as an ESLint plugin",
  "version": "1.0.38",
  "keywords": [
    "gravitywell",
    "gravitywelluk",
    "typescript-eslint",
    "eslint",
    "react-eslint",
    "react-native-eslint"
  ],
  "repository": "git@github.com:GravitywellUK/eslint-plugin.git",
  "bugs": {
    "url": "https://github.com/GravitywellUK/eslint-plugin/issues"
  },
  "author": "GravitywellUK",
  "contributors": [
    "Luke Baker <luke.baker@live.com>"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "./node_modules/.bin/tsc -b",
    "build:clean": "rm -Rf dist/ && yarn build --clean && yarn build",
    "start": "yarn build -w",
    "check:clean-diff-after-install": "git diff --quiet --exit-code",
    "lint": "eslint . --ext .js,.ts,.json",
    "lint:fix": "yarn lint --fix",
    "lint:debug": "yarn lint --debug",
    "postinstall": "yarn husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "pinst": "^2.1.6",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "eslint": "^7.32.0"
  }
}
