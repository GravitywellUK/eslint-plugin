{
  "name": "@gravitywelluk/eslint-plugin",
  "description": "Gravitywell defined ESLint rule sets as a ESLint plugin",
  "version": "1.0.24",
  "keywords": [
    "gravitywell",
    "gravitywelluk",
    "typescript-eslint",
    "eslint",
    "react-eslint",
    "react-native-eslint"
  ],
  "repository": "git@github.com:GravitywellUK/eslint-plugin.git",
  "bugs": {
    "url": "https://github.com/GravitywellUK/eslint-plugin/issues"
  },
  "author": "GravitywellUK",
  "contributors": [
    "Luke Baker <luke.baker@live.com>"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "./node_modules/.bin/tsc -b",
    "start": "yarn build -w",
    "clean": "rm -Rf dist/ && yarn build --clean",
    "check:clean-diff-after-install": "git diff --quiet --exit-code",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "lint:debug": "yarn lint --debug",
    "post-checkout": "yarn",
    "post-merge": "yarn && yarn build",
    "pre-commit": "yarn lint:fix",
    "pre-push": "yarn clean && yarn lint"
  },
  "husky": {
    "hooks": {
      "post-checkout": "yarn post-checkout",
      "post-merge": "yarn post-merge",
      "pre-commit": "yarn pre-commit",
      "pre-push": "yarn pre-push"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react-native": "^3.10.0"
  },
  "devDependencies": {
    "@gravitywelluk/eslint-plugin": "^1.0.1",
    "@types/node": "^14.11.2",
    "eslint": "^7.10.0",
    "husky": "^4.3.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "eslint": "^7.10.0"
  }
}
